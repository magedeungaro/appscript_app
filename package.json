{
  "dependencies": {
    "@types/google-apps-script": "^1.0.41",
    "parcel": "^2.0.1",
    "pug": "^3.0.2",
    "sass": "^1.43.4"
  },
  "scripts": {
    "dev": "parcel ./client/css/css.html",
    "build": "parcel build ./client/css/css.html --no-minify --no-source-maps --no-cache",
    "pug-process": "pug ./client/pug/ -o ./client/html -P",
    "bash-replace": "bash replace.sh",
    "git": "bash scripts/pu.sh",
    "process": "npm run pug-process && npm run bash-replace && npm run build && node util.js && clasp push"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.1",
    "typescript": "^4.5.2"
  }
}
